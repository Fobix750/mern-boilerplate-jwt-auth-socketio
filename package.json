{
  "name": "boilerplate-react-fullstack",
  "version": "1.0.0",
  "description": "A fullstack boilerplate",
  "main": "server/index.js",
  "scripts": {
    "start": "npm run build-client && node server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm run client",
    "build-client": "cd client && npm run build"
  },
  "keywords": [
    "boilerplate",
    "react",
    "fullstack"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.1.1",
    "http": "0.0.1-security",
    "joi": "^17.2.1",
    "mongoose": "^5.10.9",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "socket.io": "^2.3.0"
  }
}
